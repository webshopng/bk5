# This is a sample .env file
# Replace these values with your actual environment variables

# App Configuration
APP_NAME=MyApp
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=mydatabase
DB_USERNAME=root
DB_PASSWORD=

# Mail Configuration
MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

# API Keys
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here

# Other Settings
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_DRIVER=sync

